<UserControl x:Class="Radiator2000.Controls.Tabs.SelectSolidVersionControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Radiator2000.Controls.Tabs"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="510">
    <Grid>
        <Grid Grid.Column="1">
            <Label VerticalAlignment="Center" Width="180" HorizontalAlignment="Right" Grid.Column="0">Выберите версию SolidWorks:</Label>
            <ComboBox Name="solidVersionsComboBox" Height="30" Width="100" HorizontalAlignment="Left" VerticalAlignment="Center"  Grid.Column="1" SelectionChanged="solidVersionsComboBox_SelectionChanged"></ComboBox>
            <Grid.ColumnDefinitions>
                <ColumnDefinition></ColumnDefinition>
                <ColumnDefinition></ColumnDefinition>
            </Grid.ColumnDefinitions>
        </Grid>

    </Grid>
</UserControl>
